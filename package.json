  "bugs": {
    "url": "https://github.com/WhiteDevilMan/blog-fullstack-app/issues"
  },
  "homepage": "https://github.com/WhiteDevilMan/blog-fullstack-app#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "git-flow": "^0.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
